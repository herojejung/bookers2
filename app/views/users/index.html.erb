<%= render 'books/book', user: @user %>

<div class="col-md-8 offset-md-1">
  <h2>Users</h2>
    <table class="table table-hover table-inverse">
      <thead>
        <tr>
          <th></th>
          <th>images</th>
          <th>name</th>
          <th></th>
        </tr>
      </thead>
    <tbody>
  <% @users.each do |users| %>
    <tr>
      <td>
        <%= image_tag users.get_image %>
        </td>
         <td>
          <%= users.name %>
        </td>
      <td>
        <%= link_to "Show", user_path(users) %>
      </td>
    </tr>
<% end %>
</tbody>
</table>
</div>
</div>
</div>